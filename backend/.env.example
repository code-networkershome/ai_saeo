# Core API Keys (Required)
OPENAI_API_KEY=your_openai_api_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Supabase Configuration (Required)
SUPABASE_URL=your_supabase_project_url
SUPABASE_KEY=your_supabase_service_role_key

# Application Settings
APP_NAME="SAEO.ai - Autonomous SE Intelligence"
APP_VERSION="1.0.0"
DEBUG=false
API_PREFIX=/api/v1

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Optional: Performance & Research APIs
PAGESPEED_API_KEY=your_google_pagespeed_api_key
OPENPAGERANK_API_KEY=your_openpagerank_api_key_here

# Google OAuth Configuration (JSON string from google-credentials.json)
GOOGLE_CREDENTIALS_JSON=
# The callback URL (Must match your Google Cloud Console settings)
GOOGLE_REDIRECT_URI=http://localhost:8000/api/v1/auth/google/callback
FRONTEND_URL=http://localhost:5173
